<header class="head-two form-header">
</header>
<div class="browse-page">
  <h1>BROWSE THE MARKET</h1>
  <div class="browse-content">
    <div class="browse-tags">
      <% if @tags %>
        <ul>
          <% @tags.each do |tag| %>
          <li class="browse-tag"><a href="/tags/<%= tag.id %>"><%= tag.name %></a></li>
          <% end %>
        </ul>
      <% end %>
    </div>
  <!-- GRIDDED SKILLS -->
  <section id="featured">
    <% if @skills.any? %>
        <% @skills.each do |skill| %>
          <div class="featured-image main">
            <img class="browse_img" src="/img/<%=skill.id%>.jpg" alt="<%=skill.id%>"/>
            <a href="/user/profile/<%=skill.user.id %>"><div class="overlay">
              <p><%= skill.title %></p>
              </div></a>
            </div>
        <% end %>
    <% end %>
  </section>
</div>
